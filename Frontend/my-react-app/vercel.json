
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {"key": "Content-Security-Policy", "value": "default-src 'self' 'unsafe-inline' 'unsafe-eval'"},
        {"key": "Cross-Origin-Opener-Policy", "value": "same-origin"}
      ]
    }
  ],
  "rewrites": [{"source": "/(.*)", "destination": "/index.html"}]
}
